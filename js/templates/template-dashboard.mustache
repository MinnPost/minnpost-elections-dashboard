<div class="dashboard cf grid-parent {{ classes }}">

  <div class="grid-100 location-search-section">
      <form>
        <div class="location-search-group">
          <input type="text" id="address-search" placeholder="Search election results by address" />

          {{#geolocationEnabled}}
            <div class="geolocation">
              <a href="#location">Or view contests at your current location</a>
            </div>
          {{/geolocationEnabled}}
        </div>

        <input on-tap="addresssSearch" type="submit" value="Go" class="address-search-submit" />
      </form>
  </div>

  <div class="grid-50">
    <div class="contest-minneapolis-mayor">
      <h4>Minnesapolis Mayor</h4>

    </div>
  </div>

  <div class="grid-50">
    <div class="location-search">
      <h4>Elections search</h4>

      <form>
        <label for="contest-search">Search contests by title or candidate.  Start typing to see suggestions. <br /></label>
        <input type="text" id="contest-search" />
      </form>
    </div>
  </div>
</div>